{
  "manifest_version": 3,
  "name": "Bitcare Photo Downloader",
  "version": "1.0",
  "description": "Automatically load and download all photos from Bitcare.",
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["https://app.bitcare.com/contacts/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
